{"ast":null,"code":"import TablaPersonas from '@/components/TablaPersonas.vue';\nimport FormularioPersona from '@/components/FormularioPersona.vue';\nexport default {\n  name: 'app',\n  components: {\n    TablaPersonas,\n    FormularioPersona\n  },\n  data() {\n    return {\n      personas: [{\n        id: 1,\n        nombre: 'Jon',\n        apellido: 'Nieve',\n        email: 'jon@email.com'\n      }, {\n        id: 2,\n        nombre: 'Tyrion',\n        apellido: 'Lannister',\n        email: 'tyrion@email.com'\n      }, {\n        id: 3,\n        nombre: 'Daenerys',\n        apellido: 'Targaryen',\n        email: 'daenerys@email.com'\n      }]\n    };\n  },\n  methods: {\n    agregarPersona(persona) {\n      let id = 0;\n      if (this.personas.length > 0) {\n        id = this.personas[this.personas.length - 1].id + 1;\n      }\n      this.personas = [...this.personas, {\n        ...persona,\n        id\n      }];\n    },\n    eliminarPersona(id) {\n      this.personas = this.personas.filter(persona => persona.id !== id);\n    },\n    actualizarPersona(id, personaActualizada) {\n      this.personas = this.personas.map(persona => persona.id === id ? personaActualizada : persona);\n    }\n  }\n};","map":{"version":3,"names":["TablaPersonas","FormularioPersona","name","components","data","personas","id","nombre","apellido","email","methods","agregarPersona","persona","length","eliminarPersona","filter","actualizarPersona","personaActualizada","map"],"sources":["/home/david/Cliente/libreria-vue/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\" class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-md-12\">\n        <h1>Personas</h1>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-md-12\">\n        <formulario-persona @add-persona=\"agregarPersona\" />\n        <tabla-personas :personas=\"personas\" @delete-persona=\"eliminarPersona\" @actualizar-persona=\"actualizarPersona\"/>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import TablaPersonas from '@/components/TablaPersonas.vue'\n  import FormularioPersona from '@/components/FormularioPersona.vue'\n\n  export default {\n    name: 'app',\n    components: {\n      TablaPersonas,\n      FormularioPersona,\n    },\n    data() {\n      return {\n        personas: [\n          {\n            id: 1,\n            nombre: 'Jon',\n            apellido: 'Nieve',\n            email: 'jon@email.com',\n          },\n          {\n            id: 2,\n            nombre: 'Tyrion',\n            apellido: 'Lannister',\n            email: 'tyrion@email.com',\n          },\n          {\n            id: 3,\n            nombre: 'Daenerys',\n            apellido: 'Targaryen',\n            email: 'daenerys@email.com',\n          },\n        ],\n      }\n    },\n    methods: {\n      agregarPersona(persona) {\n        let id = 0;\n    \n        if (this.personas.length > 0) {\n          id = this.personas[this.personas.length - 1].id + 1;\n        }\n    \n        this.personas= [...this.personas, { ...persona, id}];\n      },\n      eliminarPersona(id) {\n        this.personas = this.personas.filter(\n        persona => persona.id !== id\n        );\n      },\n      actualizarPersona(id, personaActualizada) {\n        this.personas = this.personas.map(persona =>\n        persona.id === id ? personaActualizada : persona\n        )\n      }\n    }\n  }\n</script>\n\n<style>\n  button {\n    background: #009435;\n    border: 1px solid #009435;\n  }\n</style>\n"],"mappings":"AAiBE,OAAOA,aAAY,MAAO,gCAA+B;AACzD,OAAOC,iBAAgB,MAAO,oCAAmC;AAEjE,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,aAAa;IACbC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,CAAC;QACLC,MAAM,EAAE,KAAK;QACbC,QAAQ,EAAE,OAAO;QACjBC,KAAK,EAAE;MACT,CAAC,EACD;QACEH,EAAE,EAAE,CAAC;QACLC,MAAM,EAAE,QAAQ;QAChBC,QAAQ,EAAE,WAAW;QACrBC,KAAK,EAAE;MACT,CAAC,EACD;QACEH,EAAE,EAAE,CAAC;QACLC,MAAM,EAAE,UAAU;QAClBC,QAAQ,EAAE,WAAW;QACrBC,KAAK,EAAE;MACT,CAAC